import java.util.ArrayList;
import java.util.List;

public class Test 
{
	public static void main(String[] args) 
	{
		List<Student> list = new ArrayList<Student>();
		list.add(new Student("Abhi", "IT", 600000));
		list.add(new Student("Akash", "Markating", 400000));
		list.add(new Student("Ravi", "sale", 700000));
		list.add(new Student("Ansh", "Digital Markating", 900000));
		
		Student m = list.get(0);
		for(int i=1; i < list.size(); i++)
		{
			m= list.get(i);
		}
		System.out.println(m);
	}

}
